{
  "name": "guildmaster",
  "version": "0.0.1",
  "description": "A CMS for job search services",
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://github.com/phanect/guildmaster",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phanect/guildmaster.git"
  },
  "bugs": {
    "url": "https://github.com/phanect/guildmaster/issues"
  },
  "keywords": [
    "jobsearch",
    "joblist",
    "jobhunt",
    "jobs",
    "careers",
    "hiring"
  ],
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "lint": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && eslint .",
    "postinstall": "npm run postinstall:migrate && npm run postinstall:seed",
    "postinstall:migrate": "prisma migrate dev",
    "postinstall:seed": "esno ./scripts/postinstall-seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0"
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "^1.7.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@tailwindcss/forms": "^0.5.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "autoprefixer": "^10.4.14",
    "dayjs": "^1.11.11",
    "eslint": "^8.28.0",
    "eslint-config-phanective": "latest",
    "esno": "^4.7.0",
    "postcss": "^8.4.23",
    "postcss-load-config": "^4.0.1",
    "prisma": "^4.15.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess": "^5.0.3",
    "sveltekit-superforms": "^1.0.0",
    "tailwindcss": "^3.3.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "zod": "^3.21.4"
  },
  "engines": {
    "node": ">=18.x",
    "npm": ">=10.x"
  }
