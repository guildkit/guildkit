{
  "name": "guildkit",
  "version": "0.0.1-prealpha",
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "scripts": {
    "dev": "pnpm sync && next dev",
    "start": "next start",
    "db": "psql --username=guildkit --dbname=guildkit --host=localhost --port=5432",
    "resetdb": "nu ./scripts/scripts.nu resetdb",
    "resetdb-demo": "jiti ./scripts/resetdb-demo.ts && drizzle-kit migrate && jiti ./scripts/seed.ts",
    "build": "pnpm sync && next build",
    "lint": "pnpm sync && tsc --noEmit && eslint",
    "fix": "pnpm sync && eslint --fix",
    "clean": "git clean -dX --exclude=\"!.env\" --force",
    "refresh": "pnpm run clean && rm --force ./pnpm-lock.yaml && corepack up && pnpm install",
    "sync": "nu ./scripts/scripts.nu sync",
    "prepare": "nu ./scripts/scripts.nu sync --seed"
  },
  "dependencies": {
    "better-auth": "^1.3.9",
    "drizzle-orm": "^0.44.5",
    "next": "15.5.2",
    "pg": "^8.14.1",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.9",
    "@next/eslint-plugin-next": "^15.5.2",
    "@phanect/configs": "latest",
    "@phanect/lint": "latest",
    "@phanect/lint-react": "latest",
    "@phanect/utils": "^1.5.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dayjs": "^1.11.11",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.35.0",
    "fast-xml-parser": "^5.2.1",
    "jiti": "^2.4.2",
    "neonctl": "^2.14.0",
    "nushell": "^0.107.0",
    "postcss": "^8.4.23",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.0.0",
    "zod": "^4.1.5"
  },
  "engines": {
    "node": "20.x || 22.x || >=24.x",
    "pnpm": ">=10.x"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
