{
  "name": "guildkit",
  "version": "0.0.1-prealpha",
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "scripts": {
    "dev": "pnpm sync && vite dev",
    "db": "psql --username=guildkit --dbname=guildkit --host=localhost --port=5432",
    "resetdb": "nu ./scripts/scripts.nu resetdb",
    "resetdb-demo": "jiti ./scripts/resetdb-demo.ts && drizzle-kit migrate && jiti ./scripts/seed.ts",
    "build": "pnpm sync && vite build",
    "lint": "pnpm sync && svelte-check --tsconfig ./tsconfig.json && eslint && markuplint \"**/*.html\"",
    "fix": "pnpm sync && eslint --fix",
    "clean": "git clean -dX --exclude=\"!.env\" --force",
    "refresh": "pnpm run clean && rm --force ./pnpm-lock.yaml && corepack up && pnpm install",
    "sync": "nu ./scripts/scripts.nu sync",
    "prepare": "nu ./scripts/scripts.nu sync --seed"
  },
  "dependencies": {
    "better-auth": "^1.3.7",
    "drizzle-orm": "^0.44.2",
    "pg": "^8.14.1"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.7",
    "@markuplint/ml-config": "^4.8.10",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.86.2",
    "@phanect/configs": "latest",
    "@phanect/lint": "latest",
    "@phanect/lint-svelte": "latest",
    "@phanect/utils": "^1.5.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.33.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.4",
    "dayjs": "^1.11.11",
    "deepmerge": "^4.3.1",
    "dotenv": "^17.0.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.30.1",
    "fast-xml-parser": "^5.2.1",
    "jiti": "^2.4.2",
    "markuplint": "^4.11.8",
    "neonctl": "^2.14.0",
    "nushell": "^0.106.1",
    "sass": "^1.86.1",
    "svelte": "^5.38.2",
    "svelte-check": "^4.1.4",
    "svelte-preprocess": "^6.0.3",
    "sveltekit-superforms": "^2.23.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^7.1.2",
    "vite-plugin-devtools-json": "^1.0.0",
    "zod": "^3.25.75"
  },
  "engines": {
    "node": "20.x || 22.x || >=24.x",
    "pnpm": ">=10.x"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
