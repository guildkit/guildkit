{
  "name": "guildkit",
  "version": "0.0.1-prealpha",
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "scripts": {
    "dev": "pnpm run sync && pnpm db:restart && jiti ./scripts/build-db-seed.ts && vite dev && pnpm db:stop",
    "db": "pnpm db:restart && psql --username=guildkit --dbname=guildkit && pnpm db:stop",
    "dbsu": "pnpm db:restart && psql --username=postgres --dbname=postgres && pnpm db:stop",
    "dbrefresh": "pnpm db:stop && rm -rf ./tmp/pgdata && pnpm run refresh",
    "db:restart": "pg_ctl --pgdata=./tmp/pgdata restart || pg_ctl --pgdata=./tmp/pgdata start",
    "db:stop": "pg_ctl --pgdata=./tmp/pgdata stop || :",
    "build": "pnpm run sync && vite build",
    "lint": "pnpm run sync && svelte-check --tsconfig ./tsconfig.json && eslint && markuplint \"**/*.html\"",
    "fix": "pnpm run sync && eslint --fix",
    "clean": "git clean -dX --exclude=\"!.env\" --exclude=\"!tmp/pgdata\" --force",
    "refresh": "pnpm run clean && rm --force ./pnpm-lock.yaml && corepack up && pnpm install",
    "sync": "pnpm db:restart && svelte-kit sync && prisma migrate dev && pnpm db:stop",
    "prepare": "bash ./scripts/prepare-pg-deps.sh && bash ./scripts/prepare-pg-install.sh && pnpm run sync"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.14.1"
  },
  "devDependencies": {
    "@markuplint/ml-config": "^4.8.10",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.86.2",
    "@phanect/configs": "latest",
    "@phanect/lint": "latest",
    "@phanect/lint-svelte": "latest",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/kit": "^2.17.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/postcss": "^4.0.7",
    "@tailwindcss/vite": "^4.0.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "dayjs": "^1.11.11",
    "deepmerge": "^4.3.1",
    "eslint": "^9.11.0",
    "jiti": "^2.4.2",
    "markuplint": "^4.11.8",
    "postcss": "^8.4.23",
    "postcss-load-config": "^6.0.1",
    "prisma": "^6.3.0",
    "sass": "^1.86.1",
    "svelte": "^5.19.7",
    "svelte-check": "^4.1.4",
    "svelte-preprocess": "^6.0.3",
    "sveltekit-superforms": "^2.23.1",
    "tailwindcss": "^4.0.7",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^6.0.11",
    "zod": "^3.21.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
      "es5-ext",
      "esbuild",
      "prisma",
      "svelte-preprocess"
    ]
  },
  "engines": {
    "node": "22.x",
    "pnpm": ">=10.x"
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808"
}
