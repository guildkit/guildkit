{
  "name": "guildkit",
  "version": "0.0.1-prealpha",
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "scripts": {
    "dev": "pnpm sync && vite dev",
    "db": "psql --username=guildkit --dbname=guildkit --host=localhost --port=5432",
    "resetdb": "nu ./scripts/scripts.nu resetdb",
    "resetdb:demo": "neon branches reset \"preview/${VERCEL_GIT_COMMIT_REF}\" --parent --project-id=\"${NEON_PROJECT_ID}\" && drizzle-kit migrate && jiti ./scripts/seed.ts",
    "build": "pnpm sync && vite build",
    "release": "pnpm sync && drizzle-kit migrate",
    "lint": "pnpm sync && svelte-check --tsconfig ./tsconfig.json && eslint && markuplint \"**/*.html\"",
    "fix": "pnpm sync && eslint --fix",
    "clean": "git clean -dX --exclude=\"!.env\" --force",
    "refresh": "pnpm run clean && rm --force ./pnpm-lock.yaml && corepack up && pnpm install",
    "sync": "nu ./scripts/scripts.nu sync",
    "prepare": "nu ./scripts/scripts.nu sync --seed"
  },
  "dependencies": {
    "better-auth": "^1.3.4",
    "drizzle-orm": "^0.44.2",
    "pg": "^8.14.1"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.4",
    "@markuplint/ml-config": "^4.8.10",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.86.2",
    "@phanect/configs": "latest",
    "@phanect/lint": "latest",
    "@phanect/lint-svelte": "latest",
    "@phanect/utils": "^1.5.0",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.17.1",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/postcss": "^4.0.7",
    "@tailwindcss/vite": "^4.0.7",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.4",
    "dayjs": "^1.11.11",
    "deepmerge": "^4.3.1",
    "dotenv": "^17.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.30.1",
    "fast-xml-parser": "^5.2.1",
    "jiti": "^2.4.2",
    "markuplint": "^4.11.8",
    "neonctl": "^2.14.0",
    "nushell": "^0.106.1",
    "postcss": "^8.4.23",
    "postcss-load-config": "^6.0.1",
    "sass": "^1.86.1",
    "svelte": "^5.35.4",
    "svelte-check": "^4.1.4",
    "svelte-preprocess": "^6.0.3",
    "sveltekit-superforms": "^2.23.1",
    "tailwindcss": "^4.0.7",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^7.1.2",
    "vite-plugin-devtools-json": "^0.4.1",
    "zod": "^3.25.75"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@prisma/client",
      "@prisma/engines",
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "prisma",
      "svelte-preprocess",
      "unrs-resolver"
    ]
  },
  "engines": {
    "node": "20.x || 22.x || >=24.x",
    "pnpm": ">=10.x"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
