{
  "name": "guildkit",
  "version": "0.0.1-prealpha",
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "scripts": {
    "dev": "pnpm sync && next dev",
    "start": "next start",
    "db": "psql --username=guildkit --dbname=guildkit --host=localhost --port=5432",
    "resetdb": "nu ./scripts/scripts.nu resetdb",
    "resetdb-demo": "jiti ./scripts/resetdb-demo.ts && drizzle-kit migrate && jiti ./scripts/seed.ts",
    "build": "pnpm sync && next build",
    "lint": "pnpm sync && tsc --noEmit && eslint",
    "fix": "pnpm sync && eslint --fix",
    "clean": "git clean -dX --exclude=\"!.env\" --force",
    "refresh": "pnpm run clean && rm --force ./pnpm-lock.yaml && corepack up && pnpm install",
    "sync": "nu ./scripts/scripts.nu sync",
    "prepare": "nu ./scripts/scripts.nu sync --seed"
  },
  "dependencies": {
    "better-auth": "^1.3.7",
    "drizzle-orm": "^0.44.5",
    "next": "15.5.0",
    "pg": "^8.14.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.3.7",
    "@phanect/configs": "latest",
    "@phanect/lint": "latest",
    "@phanect/lint-svelte": "latest",
    "@phanect/utils": "^1.5.0",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dayjs": "^1.11.11",
    "dotenv": "^17.0.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-seed": "^0.3.1",
    "eslint": "^9.30.1",
    "fast-xml-parser": "^5.2.1",
    "jiti": "^2.4.2",
    "neonctl": "^2.14.0",
    "nushell": "^0.106.1",
    "sass": "^1.91.0",
    "typescript": "^5.0.0",
    "zod": "^3.25.75"
  },
  "engines": {
    "node": "20.x || 22.x || >=24.x",
    "pnpm": ">=10.x"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
