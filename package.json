{
  "name": "guildkit",
  "version": "0.0.1",
  "author": "Jumpei Ogawa (https://phanective.org)",
  "license": "SEE LICENSE IN LICENSE.md",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run --recursive dev",
    "build": "pnpm run --recursive build",
    "lint": "pnpm run --recursive lint && tsc --noEmit && eslint",
    "fix": "eslint --fix",
    "clean": "git clean -dX --exclude=\"!.env\" --exclude=\"!*.db\" --force",
    "refresh": "pnpm run clean && rm --force ./pnpm-lock.yaml && corepack up && pnpm install"
  },
  "devDependencies": {
    "@phanect/configs": "latest",
    "@phanect/lint": "latest",
    "@phanect/lint-svelte": "latest",
    "eslint": "^9.11.0",
    "jiti": "^2.4.2",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": "22.x",
    "pnpm": ">=10.x"
  },
  "packageManager": "pnpm@10.5.1+sha512.c424c076bd25c1a5b188c37bb1ca56cc1e136fbf530d98bcb3289982a08fd25527b8c9c4ec113be5e3393c39af04521dd647bcf1d0801eaf8ac6a7b14da313af"
}
